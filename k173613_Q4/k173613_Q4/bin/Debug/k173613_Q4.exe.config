<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<!-- Path Where XML File Store -->
		<add key="RecordsPathXML" value="D:\All-PSX-Records\"/>
		<!-- Path Where JSON File to be Store -->
		<add key="RecordsPathJSON" value="D:\All-PSX-Records-JSON\"/>

		<!-- Log File For Errors -->
		<add key="Log-File" value="D:/Log-File.txt"/>
	</appSettings>
</configuration>